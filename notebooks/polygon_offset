let
	pa = PolyRegion(; domain=[LatLon(10Â°, -5Â°), LatLon(10Â°, 15Â°), LatLon(27Â°, 15Â°), LatLon(27Â°, -5Â°)])

	poly = pa.domain.latlon
	
	intPoly = map([vertices(poly)...]) do vertex
        y = get_lat(vertex) |> ustrip 
        x = get_lon(vertex) |> ustrip
        IntPoint(x, y, 3, 7) # Consider LON as X and LAT as Y
    end
end

